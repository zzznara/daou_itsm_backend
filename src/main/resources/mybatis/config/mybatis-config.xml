<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false"/>
		<setting name="multipleResultSetsEnabled" value="true"/>
		<setting name="useGeneratedKeys" value="false"/>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="logPrefix" value="[mybatis]"/>
	</settings>


    <typeAliases>
        <typeAlias type="java.util.HashMap"
				   alias="hashMap"/>
		<typeAlias type="java.util.Map"
				   alias="map"/>
		<typeAlias type="java.util.LinkedHashMap"
				   alias="linkedHashMap"/>
		<typeAlias type="java.lang.String"
				   alias="string"/>
		<typeAlias type="java.lang.Integer"
				   alias="int"/>
		<typeAlias type="java.lang.Long"
				   alias="long"/>
		<typeAlias type="java.lang.Boolean"
				   alias="boolean"/>
		<typeAlias type="java.math.BigDecimal"
				   alias="bigDecimal"/>
		<typeAlias type="java.util.Date"
				   alias="date"/>
		<typeAlias type="kr.co.dysnt.framework.common.util.LowerCamelHashMap"
				   alias="camelMap"/>
		<typeAlias type="org.egovframe.rte.psl.dataaccess.util.EgovMap"
		           alias="egovMap"/>
    </typeAliases>
</configuration>
